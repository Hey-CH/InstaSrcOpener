{
	"name": "InstaSrcOpener",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"description": "Instagram source open new tab.",
	"permissions": ["tabs", "contextMenus"],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [{
		"matches": [
			"https://www.instagram.com/*"
		],
		"js": [
			"jquery-3.6.0.min.js",
			"content_scripts.js"
		]
	}]
}